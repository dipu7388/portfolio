    "@types/chai": "^4.2.21",
    "@types/compression": "^1.7.1",
    "@types/concurrently": "^6.2.1",
    "@types/connect-mongo": "^3.1.3",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^7.28.0",
    "@types/express": "^4.17.13",
    "@types/express-flash": "0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/lodash": "^4.14.172",
    "@types/lusca": "^1.7.1",
